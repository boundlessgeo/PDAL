l."PCBLK_MIN_RES", l."PCBLK_MAX_RES", l."NUM_POINTS",
l."NUM_UNSORTED_POINTS", l."PT_SORT_DIM", l."POINTS", b.cloud
FROM SIMPLE_BLOCKS l, SIMPLE_CLOUD b
